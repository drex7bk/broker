			<xsd:element name="comentarios" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="ArrayOfComentario">
		<xsd:sequence>
			<xsd:element name="comentario" type="this:ComentarioType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	
</xsd:schema>